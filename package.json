    "packageDarwin2": "electron-packager . 'spring-festival' --platform=darwin --overwrite --icon=images/tora-ski-768x768.icns --download.mirrorOptions.mirror=https://npm.taobao.org/mirrors/electron/ --arch=x64 --out=./dist --asar --app-version=1.0.0",
